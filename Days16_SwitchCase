package melan;

public class Melan{
    public static void main(String[] args) {

        System.out.print("Jumlah MK saya disemester 1 = ");
        
        int jumlah = 8;
        switch (jumlah){
            case 8 :
            System.out.println("8");
            break;
            case 9 :
            System.out.print("9");
        }   
    }       
}
