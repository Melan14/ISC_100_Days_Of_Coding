package Melan;
import java.util.Scanner;

public class Melan{
    public static void main(String[] args) {
      
       int a = 10;
       int b = 5;
        
        a +=b;
        a -=b;
        System.out.println(a);
        b *=a;
        System.out.println(b);
        b /=a;
        System.out.println(b);
        a %=b;
        System.out.println(a);
          
    }
}
